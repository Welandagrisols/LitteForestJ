{
  "expo": {
    "name": "LittleForest",
    "slug": "littleforest-app",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./public/icon-512x512.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/icon-512x512.png",
      "resizeMode": "contain",
      "backgroundColor": "#22A45D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.agrisols.littleforest"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/icon-512x512.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.agrisols.littleforest",
      "versionCode": 1
    },
    "web": {
      "favicon": "./public/icon-192x192.png",
      "bundler": "metro"
    },
    "plugins": [],
    "scheme": "littleforest",
    "newArchEnabled": true
  }
}
